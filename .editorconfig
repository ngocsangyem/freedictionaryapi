# http://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = tab
indent_size = 4
tab_width = 4
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = false
quote_type = single

[*.js]
indent_style = tab
indent_size = 4
tab_width = 4

[{package.json,.travis.yml}]
indent_style = tab
indent_size = 4
tab_width = 4

[**.md]
insert_final_newline = false
trim_trailing_whitespace = false
